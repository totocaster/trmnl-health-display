<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.toto.trmnlhealth</string>

    <key>ProgramArguments</key>
    <array>
      <string>/bin/bash</string>
      <string>/Users/toto/Developer/trmnl_health/scripts/run_dashboard.sh</string>
    </array>

    <key>StartCalendarInterval</key>
    <dict>
      <key>Hour</key>
      <integer>9</integer>
      <key>Minute</key>
      <integer>30</integer>
    </dict>

    <key>KeepAlive</key>
    <dict>
      <key>SuccessfulExit</key>
      <false/>
    </dict>

    <key>StandardOutPath</key>
    <string>/Users/toto/Library/Logs/trmnl_health.launchd.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/toto/Library/Logs/trmnl_health.launchd.log</string>

    <key>ThrottleInterval</key>
    <integer>3600</integer>
  </dict>
</plist>
